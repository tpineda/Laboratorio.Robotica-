int led = 13;
void setup () {
  //put your setup code here, to run ince;
  pinMode(led, OUTPUT)
  Serial.begin(9000);
}

void loop() {
  //put your main code here, to run repeatedly;
  
  for (int j=1; J<=50; j++)
  {
    digitalWrite(led,225);
    delay(1000/j);
    digitalWrite(led,LOW);
    delay(1000/j);
  }
  }
  
  
